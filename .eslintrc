{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "plugins": [
    "import"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "ignorePatterns": [
    "dist",
    "node_modules",
    "webpack.*",
    "config/paths.js"
  ],
  "root": true,
  "rules": {
    "import/prefer-default-export": "off"
  }
}
